.loginview {
    width: 462rpx;
    height: 120rpx;
    color: #fff;
    display: block;
    margin: 40rpx auto;
    line-height: 0;
    padding: 0;
    background: none;
    position: relative;
    margin-top: 200rpx;
}

.loginview>image {
    width: 100%;
    height: 100%;
}

.loginview>text {
    position: absolute;
    top: 0;
    left: 0;
    width: 462rpx;
    height: 120rpx;
    color: #fff;
    display: block;
    text-align: center;
    line-height: 120rpx;
}


