.steps {
  position:absolute;
  bottom: 64px;
  counter-reset: step;/*创建步骤数字计数器*/
  width: 100vw;

}
/*步骤描述*/
.steps view {
  list-style-type: none;
  font-size: 12px;
  text-align: center;
  width: 14.2%;
  position: relative;
  color: #fff;
  z-index: 2;
  float: left;

}
 /*步骤数字*/
.steps view:before {
  display: block;
  content: counter(step);/*设定计数器内容*/
  counter-increment: step;/*计数器值递增*/
  width: 15px;
  height: 15px;
  background-color: white;
  line-height: 15px;
  border-radius: 15px;
  font-size: 12px;
  color: #fff;
  text-align: center;
  font-weight: 700;
  margin: 0 auto 8px auto;
  
}



/*连接线*/
.steps view:after {
  content: '';
  width: 100%;
  height: 2px;
  background-color: white;
  position: absolute;
  left: 0;
  top: 7px;
  z-index: -1;/*放置在数字层后面*/
}
/*连接线放置在数字层后面*/
.steps view:first-child {
  z-index: 3;
}
.steps view:last-child {
  z-index: 1;
}
/*去掉第一步多出的连接线*/
.steps view:first-child:after {
  width: 50%;
  left: 50%;
}
/*去掉最后一步多出的连接线*/
.steps view:last-child:after {
  width: 50%;
}
/*完成步骤变绿*/
.steps view.active:before {
  /* background-color: #019875; */
  content: url(data:image/png;base64,); 
 
}
.steps view.active {
  color: white;


}
.steps view.active:before,
.steps view.active:after {
  background-color: white;


}

