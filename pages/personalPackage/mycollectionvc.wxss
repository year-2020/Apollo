@import "/pages/personalPackage/template/collectioncell.wxss";
page{
  background-color: #F2F2F2;
}
.searchView {
  border-radius: 20px;
  background-color: white;
  color: rgba(179, 179, 180, 1);
  margin: 20rpx 40rpx;
  display:flex; 
  justify-content:center;
   align-items:center;
   height: 30px;
}

.searchViewC {
    width: 150px;
    height: 30px;   
    /* background-color:red; */
    color: rgba(179, 179, 180, 1);
}
.searchIcon{
    position: absolute;
  margin-top: 6px;
  width: 18px;
  height: 18px;
}
.plast{
  height: 100%;
  /* background: yellow; */
   margin-left: 20px;
   font-size: 14px;
   display: flex;
align-items: center;
justify-content: center;
}
.tags-view{
  background-color: white;
  height: 90px;
  margin: 20rpx 40rpx;
  display:flex; 
  border-radius: 8px;

}
.tags-container{
  height: 50px;
  width: 244px;
  margin-left: auto;
  margin-right: auto;
  background-color: white;
  margin-top: 20px;
   display: flex;   
}

.child{   
    flex:1;   
    height: 50px;   
    line-height: 50px;
    text-align: center;
}   
.child + .child{   
    margin-left: 20px;   
}   

.col-line{
  height: 35px;
  width: 1px;
  /* background-color: #3E7DFB;   */
  margin-top: 8px;
  border-right: 1px dashed #006dcc;
}

.hx01{
  text-align: center;
   height: 25px;
   line-height: 25px;
   flex: 1;
   font-size: 14px;
  /* 文字不换行*/
  overflow:hidden;text-overflow:ellipsis; white-space:nowrap;
  position: absolute;
  width: 60px;
  /* background-color: yellow; */
  color: #3E7DFB;
}
.hx1{
  text-align: center;
  height: 25px;
  flex: 1;
  font-size: 14px;
  margin-top: 25px;
  position: absolute;
  width: 60px;
  /* background-color: gainsboro; */
  line-height: 25px;
  color: #3E7DFB;
}




.no-data{
  text-align: center;
  width:100%;
