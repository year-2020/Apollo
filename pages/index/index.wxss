.icon-wrap {
  width: 25%;
  height: 60px;
  /* float: left; */

  text-align: center;
}

.tab-icon {
  font-size: 24px;
  padding: 0px;
  display: inline-block;
  color: rgba(69, 90, 100, .8);
}
.icon-wrap:nth-child(4) .icon-classname{
  position: relative;
}
.icon-wrap:nth-child(4) .icon-classname::after{
  content: "";
  display: block;
  width: 8px;
  height: 8px;
  background: #f00;
  border-radius: 100%;
  position: absolute;
  left: 50%;
  top: -4px;
  margin-left: 25px;
}
