.videoList01 {
    height: 220px;
    box-shadow: 0px 3px 6px 0px rgba(71,71,71,0.1);
    border-radius: 10rpx;
    margin: 0 30rpx 30rpx 30rpx;
    overflow: hidden;
    clear: both;
    width: 100%;
}

.videoList01>.videoListBox {
    width: 100%;
    height: 220px;
    border-radius: 10rpx;
    overflow: hidden;
}

.videoList01 .videoListImg {
    width: 100%;
    height: 167px;
    border-radius: 10rpx 10rpx 0 0;
}
.videoList{
  display: flex;
  width: 48%;
  
}
.videoList>.videoListBox {
    width: 165px;
    height: 187px;
    box-shadow: 0px 3px 6px 0px rgba(71,71,71,0.1);
    border-radius: 10rpx;
    margin: 0 0 30rpx 30rpx;
    float: left;
    overflow: hidden;
}

.videoListImg {
    width: 100%;
    height: 107px;
    border-radius: 10rpx 10rpx 0 0;
}

.videoListImg>image {
    border-radius: 10rpx 10rpx 0 0;
}

.videoListImg>image {
    width: 100%;
    height: 100%;

}

.videoListContent {
    display: block;
    padding: 16rpx 40rpx 0 16rpx;
    word-wrap: break-word;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    color: #333333;
    font-size: 26rpx;
}

.videoListico {
    position: relative;
    margin-top: 20rpx;
    padding: 0 16rpx;
    line-height: 1;
}

.videoListico>text {
    display: block;
}

.ion {
    position: absolute;
}

.videoicon {
    width: 30rpx;
    height: 30rpx;
    top: 0;
}

.po {
    width: 33rpx;
    height: 28rpx;
    top: 1rpx;
    left: 170rpx;
}

.videotext {
    left: 60rpx;
}

.num {
    font-size: 24rpx;
    width: 100rpx;
    top: 0;
    color: #999;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    line-height: 30rpx;
}

.commentNum {
    left: 218rpx;
}

.clea:after {
    display: block;
    content: '';
    clear: both;
    height: 0;
}

.test-playico{
  position: absolute;
  width: 64px;
  height: 64px;
  margin-top: -110px;
  left: 45%;

}