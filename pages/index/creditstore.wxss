.ctab{
  width: 90%;
  height: 75rpx;
  margin: 10px;
  display:flex; 
  flex-direction: column;
  justify-content:center;
  align-items:center;
  margin-left:20px;
  margin-right:20px;

}
#affix {
    width: 60%;
    height: 75rpx;
    background: #fff;
    display:flex;
}

.scrollviewitem {
    font-size: 40rpx;
    color: #999;
    height: 100%;
    line-height: 75rpx;
    flex: 1;
    text-align: center;
}

#affix .active {
    position: relative;
}

.active::after {
    position: absolute;
    content: "";
    width: 60rpx;
    height: 4rpx;
    border-radius: 4rpx;
    left: 50%;
    bottom: 0;
    margin-left: -30rpx;
}

.table {
  display: grid;
  /* width: 96%; */
  margin-left: 20px;
  margin-right: 20px;
  overflow-x: hidden;
  /* 设置列、行大小 fr单位是等分分配列空间*/
  grid-template-columns: 1fr 1fr;
  grid-template-rows: 240px 240px;
  /* 有多余数据时，自动添加新行时默认行高为：200rpx */
  grid-auto-rows: 240px;
  /* 设置网格线大小 */
  grid-gap: 10px;
  margin-top: 20px;
}

.table-th {
  font-weight: bold;
  /* background-color: #888; */
  display: flex;
  flex-direction: column;
}

