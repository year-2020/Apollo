.goods-title{
  width: 100%;
  /* background-color: yellow; */
  height: 22px;
  font-size: 14px;
  /* 两行显示,多余的直接点点点 */
   overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}
.goods-des{
  width: 100%;
  height: 30px;
  /* background-color: purple; */
  font-size: 12px;
  line-height: 30px;
  /** 超过一行显示省略号*/
  display:-webkit-box;
  -webkit-line-clamp:1;
  overflow:hidden;
  text-overflow:ellipsis;
  -webkit-box-orient:vertical;
  word-break:break-all;
}

