.commVideoCell{
 width: 42.5%;
    height: 325rpx;
    box-shadow: 0px 3px 6px 0px rgba(71,71,71,0.1);
    border-radius: 10rpx;
    margin-top: 20px;
    /* margin: 0 0 30rpx 30rpx; */
    margin-left: 5%;
    float: left;
    overflow: hidden;
}

.commVideoImg{
    width: 100%;
    height: 200rpx;
    border-radius: 10rpx 10rpx 0 0;
}

.commVideoImg>image {
    border-radius: 10rpx 10rpx 0 0;
    width: 100%;
    height: 100%;
}
.video-title{
    display: block;
   
    padding: 5px;
    word-wrap: break-word;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    color: #333333;
    font-size: 26rpx;
}