page {
    background: #fff;
}

.Distinguish {
    padding-bottom: 106rpx;
    box-sizing: border-box;
}

.noteContent {
    min-height: 100rpx;
    border-top: 2rpx solid #dcdcdc;
    border-bottom: 2rpx solid #dcdcdc;
    background: #fff;
    padding-bottom: 30rpx;
}

.noteinfoView {
    height: 74rpx;
    padding: 30rpx;
    position: relative;
}

.noteinfoViewImg {
    height: 74rpx;
    width: 74rpx;
    border-radius: 50%;
}

.noteinfoViewName {
    font-size: 30rpx;
    color: #666;
    position: absolute;
    top: 32rpx;
    left: 120rpx;
}

.noteinfoViewtime {
    font-size: 22rpx;
    color: #999;
    position: absolute;
    top: 80rpx;
    left: 120rpx;
}

.title {
    font-size: 34rpx;
    color: #333;
    padding: 0 30rpx;
    font-weight: 700;
    line-height: 1.2;
}

.top {
    padding-top: 30rpx;
}

.centent {
    margin-top: 30rpx;
    font-size: 28rpx;
    color: #333;
    padding: 0 20rpx;
    line-height: 1.6;
    word-wrap: break-word;
    box-sizing: border-box;
}

.centent image,.centent img {
    width: 50%;
    box-sizing: border-box;
}

.browsing {
    width: 30rpx;
    height: 23rpx;
    position: absolute;
    right: 92rpx;
    top: 80rpx;
}

.browsingNum {
    position: absolute;
    right: 5rpx;
    top: 72rpx;
    color: #999;
    font-size: 24rpx;
    width: 80rpx;
}

.community {
    padding-top: 20rpx;
    position: relative;
    box-sizing: border-box;
    background: #fff;
}

.communityText {
    display: block;
    background: #fff;
    width: 100%;
    text-indent: 56rpx;
    position: relative;
    font-size: 30rpx;
}

.communityText::before {
    position: absolute;
    top: 6rpx;
    left: 30rpx;
    width: 6rpx;
    height: 32rpx;
    display: block;
    content: '';
    border-radius: 4rpx;
}

.communityVlew {
    padding: 30rpx;
    background: #fff;
    border-bottom: 2rpx solid #e5e5e5;
    position: relative;
}

.cView {
    width: 116rpx;
    height: 44rpx;
    text-align: center;
    line-height: 44rpx;
    font-size: 24rpx;
    position: absolute;
    right: 30rpx;
    top: 40rpx;
    border: 1rpx solid #5096f5;
    border-radius: 8rpx;
    color: #5096f5;
    z-index: 100;
}

.sView {
    width: 106rpx;
    height: 106rpx;
    position: absolute;
    right: 30rpx;
    top: 17rpx;
}

.communityInfo {
    height: 74rpx;
    position: relative;
}

.communityInfo>.noteinfoViewName {
    top: 4rpx;
    left: 90rpx;
}

.communityInfo>.noteinfoViewtime {
    top: 44rpx;
    left: 90rpx;
}

.communityCon {
    margin-top: 30rpx;
    padding-left: 96rpx;
}

.communityCom {
    margin: 24rpx 0 0 96rpx;
    background: #f3f4f6;
    padding: 22rpx;
    box-sizing: border-box;
    font-size: 28rpx;
}

.communityCom .comName01 {
    color: #46529b;
}

.communityCom .comName02 {
    margin: 0 18rpx;
    color: #46529b;
}

.commentcomm {
    display: flex;
    margin-top: 34rpx;
}

.commentcomm>view {
    flex: 1;
}

.commentcomm>view view {
    display: flex;
    justify-content: center;
    align-items: center;
    color: #333;
    font-size: 24rpx;
    border-right: 1rpx solid #dcdcdc;
}

.commentcomm text {
    display: block;
}

.commentcomm>view:last-child view {
    border: 0;
}

.gwige {
    width: 30rpx;
    height: 28rpx;
    display: block;
    margin-right: 24rpx;
}

.bomNav {
    position: fixed;
    bottom: 0;
    left: 0;
    height: 106rpx;
    background: #fff;
    z-index: 999;
    width: 100%;
    display: flex;
}

.bomNav>view {
    display: flex;
    justify-items: center;
    align-items: center;
}

.bomNavInut {
    background: #fff;
    width: 494rpx;
    height: 100%;
    margin: 0 30rpx;
    border-radius: 10rpx;
}

.bomNavInut>input {
    display: block;
    width: 494rpx;
    height: 70rpx;
    text-indent: 42rpx;
    background: #eee;
    border-radius: 10rpx;
}

.plac {
    color: #b1b1b1;
    font-size: 32rpx;
}

.starts {
    width: 36rpx;
    height: 34rpx;
    display: block;
    text-align: center;
    margin: 6rpx auto;
    background-position: center center;
}

.shareView {
    width: 30rpx;
    height: 28rpx;
    display: block;
    text-align: center;
    margin: 8rpx auto;
}

.bomView {
    flex: 1;
    text-align: center;
}

.bomView>view {
    width: 100%;
    text-align: center;
    position: relative;
}

.bomView>view button {
    position: absolute;
    top: 0%;
    left: 0;
    width: 100%;
    height: 100%;
    background: #000;
    opacity: 0;
}

.startsNum {
    text-align: center;
    display: block;
    font-size: 24rpx;
    width: 100%;
}

.sele {
}

.cView {
    display: none;
}

.sView {
    display: none;
}

.full {
    color: #46529b;
    font-size: 28rpx;
    margin-top: 15rpx;
}

.imghs>image {
    width: 100%;
}

.absenceView {
    margin-top: 90rpx;
}

.mome {
    width: 185rpx;
    height: 246rpx;
    margin-top: 100rpx;
}

.complete {
    background: #fff;
    color: #999;
    width: 100%;
    height: 100rpx;
    text-align: center;
    line-height: 100rpx;
    font-size: 28rpx;
}

