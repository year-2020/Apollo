.bottomButton {
    width: 93%;
    /* position: fixed; */
    /* bottom: 0; */
    left: 0;
    z-index: 10;
    height: 80rpx;
    padding: 20rpx 0;
    padding-left: 3.5%;
    padding-right: 3.5%;
    background-color: #ffffff;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
    margin-top: 20px;
    
}

.bottomButtonCenter {
    justify-content: center;
}

.bottomButtonDetail {
    padding-bottom: 50rpx;
}

.bottomButtonDetailF {
}

.bottomButton .buttonDefault {
    width: 33%;
    background: #FFFFFF;
    border: 1px solid #E3E3E3;
}

.bottomButton .buttonMiddleL {
    width: 48%;
    background: #FFFFFF;
    border: 1px solid #E3E3E3;
}

.bottomButton .buttonMiddleR {
    width: 48%;
    color: #FFFFFF;
    background: linear-gradient(51deg,#3949AB 0%,#5C6BC0 100%);
}

.bottomButton .buttonPrimaryDisabled {
    width: 63%;
    background-color: #E3E3E3;
    color: #999999;
}

.bottomButton .buttonPrimary {
    width: 63%;
    background: linear-gradient(51deg,#3949AB 0%,#5C6BC0 100%);
    color: #FFFFFF;
}

.bottomButton .buttonPrimaryBig {
    width: 100%;
    color: #FFFFFF;
    background: linear-gradient(51deg,#3949AB 0%,#5C6BC0 100%);
}

.bottomButton .buttonPrimaryBigDisabled {
    width: 100%;
    background-color: #E3E3E3;
    color: white;
}

.bottomButton .buttonDefaultBig {
    width: 100%;
    color: #333333;
    background: #FFFFFF;
    box-shadow: 0 0 4rpx 0 rgba(188,188,188,0.50);
}

.coverView {
    width: 93%;
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 0;
    height: 80rpx;
    padding-left: 3.5%;
    padding-right: 3.5%;
    padding-top: 20rpx;
    padding-bottom: 50rpx;
    display: flex!important;
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
    background-color: #FFFFFF;
}

.coverView .buttonPrimaryBig,.coverView .buttonPrimary {
    background-color: #3949AB;
    border: 1px solid #3949AB;
}

@-webkit-keyframes moveLR {
    0% {
        opacity: 0.25;
        left: -3%;
    }

    3% {
        left: 0%;
    }

    92% {
        left: 100%;
    }

    100% {
        opacity: 0;
        left: 100%;
    }
}

@keyframes moveLR {
    0% {
        opacity: 0.25;
        left: -3%;
    }

    3% {
        left: 0%;
    }

    92% {
        left: 100%;
    }

    100% {
        opacity: 0;
        left: 100%;
    }
}

.bottomButton .lineMove {
    position: absolute;
    top: 0rpx;
    height: 120rpx;
    margin-top: -20rpx;
    left: 0rpx;
    width: 30rpx;
    background-color: #ffffff;
    filter: blur(16rpx);
    transform: rotate(-25deg);
    animation: moveLR 2s linear infinite;
}
